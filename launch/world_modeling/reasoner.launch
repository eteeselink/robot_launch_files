<?xml version="1.0"?>
<launch>

    <!-- Get the machine file -->
    <arg name="machine" default="localhost"/>
    <include file="$(env ROBOT_BRINGUP_PATH)/machines/$(arg machine).machine" />

    <!-- Reasoner server -->
    <node name="reasoner_server" pkg="swi_prolog" type="swipl_server" output="log" machine="$(arg machine)"/>

</launch>
