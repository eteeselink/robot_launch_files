<?xml version="1.0"?>
<launch>

    <!-- Get the machine file -->
    <arg name="machine" default="localhost"/>
    <include file="$(env ROBOT_BRINGUP_PATH)/machines/$(arg machine).machine" />

    <arg name="robot" />
    
    <node name="action_server" pkg="action_server" type="action_server_main" output="screen" />
    <node name="action_server_py" pkg="action_server" type="main.py" args="$(arg robot)" output="screen" />

 </launch>
